		<typeAlias alias="moneySalaryType" type="com.ttsofts.entity.money.MoneySalaryType" />
		<typeAlias alias="moneySalary" type="com.ttsofts.entity.money.MoneySalary" />
		<typeAlias alias="moneyPay" type="com.ttsofts.entity.money.MoneyPay" />
		<typeAlias alias="moneyCard" type="com.ttsofts.entity.money.MoneyCard" />
	</typeAliases>
	<mappers>
		<!-- 用户管理 -->
		<mapper resource="com/ttsofts/entity/mapper/mysql/user/user_info_mapper.xml" />
		<mapper resource="com/ttsofts/entity/mapper/mysql/user/user_menu_mapper.xml" />
		<mapper resource="com/ttsofts/entity/mapper/mysql/user/user_power_mapper.xml" />
		<mapper resource="com/ttsofts/entity/mapper/mysql/user/user_info_power_mapper.xml" />
		<!-- 移动应用 -->
		<mapper resource="com/ttsofts/entity/mapper/mysql/app/app_advert_mapper.xml" />
