		<typeAlias alias="userInfoPower" type="com.ttsofts.entity.user.UserInfoPower" />
		<!-- 移动应用 -->
		<typeAlias alias="appAdvert" type="com.ttsofts.entity.app.AppAdvert" />
		<typeAlias alias="appAdvertSdk" type="com.ttsofts.entity.app.AppAdvertSdk" />
		<typeAlias alias="appInfo" type="com.ttsofts.entity.app.AppInfo" />
